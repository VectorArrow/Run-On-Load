{
    "name": "RunOnLoad",
    "version": "0.2",
    "description": "Run an inspector snippet on pageload.",
    "devtools_page": "devtools.html",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "http://*/*",
        "https://*/*"
    ],
    "manifest_version": 2
}